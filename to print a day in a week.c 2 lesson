#include<stdio.h>
int main()
{
   int n;
   printf("enter a number:");
   scanf("%d",&n);
   if(n==1)
   printf("SUNDAY\n");
   else if(n==2)
   printf("MONDAY\n");
   else if(n==2)
   printf("TUESDAY\n");
   else if(n==2)
   printf("WEDNESDAY\n");
   else if(n==2)
   printf("THURSDAY\n");
   else if(n==2)
   printf("FRIDAY\n");
   else if(n==2)
   printf("SATURDAY\n");
   else
   printf("INVALID DAY\n");
   return 0;
}
